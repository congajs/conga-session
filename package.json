    }
  ],
  "license": "MIT",
  "engines": {
    "node": "6.0.0"
  },
  "dependencies": {
    "express-session": "^1.15.3"
  },
  "devDependencies": {
    "jasmine": "^2.6.0",
    "request": "^2.81.0",
    "session-file-store": "^1.0.0",
    "@conga/framework": "^2.0.0",
    "@conga/framework-bass": "congajs/conga-bass#2.0.0-dev",
